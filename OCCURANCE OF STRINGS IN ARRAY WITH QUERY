####FIND THE OCCURANCE OF STRINGS IN ARRAY WITH QUERY

string=['aba','baba','aba','xzxb']
query=['aba','xzxb','ab']
count_string= 0
#print(count_string)
count_query=0
count=0
occur=[]
for i in range(len(query)):
    counter=0
    for j in range(len(string)):
        if query[i] == string[j]:
            counter+=1
    occur.append(counter)
print(occur)
